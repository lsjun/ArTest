<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.breeze.artest">

    <uses-feature android:glEsVersion="0x00020000" android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".square.TriangleActivty"
            android:screenOrientation="portrait" />
        <activity android:name=".square.SquareActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".sample.OpenGLES20Activity"
            android:screenOrientation="portrait" />
        <activity android:name=".modelCube.CuboActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>
